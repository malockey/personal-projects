@echo off
color 1
:logineng
cls
mode 25,7
set login==admin
set password==admin
@echo =========================
set /p login2=Login: 
@echo =========================
set /p password2=Password: 
@echo =========================
cls
@echo off
if /i %login% equ %login2% (
goto password
) else (msg * Login or password incorrect)
cls
goto logineng
:password
if /i %password% equ %password2% (
mode 61,15
cls
@echo Login...
timeout /t 1
goto inicioeng
) else (msg * Login or password incorrect)
cls
goto logineng
:inicioeng
color 1
cls
