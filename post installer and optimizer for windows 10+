Reg.exe add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\Tcpip\ServiceProvider" /v "DnsPriority" /t REG_DWORD /d "00000006" /f
Reg.exe add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\Tcpip\ServiceProvider" /v "HostsPriority" /t REG_DWORD /d "00000005" /f
Reg.exe add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\Tcpip\ServiceProvider" /v "LocalPriority" /t REG_DWORD /d "00000004" /f
Reg.exe add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\Tcpip\ServiceProvider" /v "NetbtPriority" /t REG_DWORD /d "00000007" /f
cls
msg * Optimize internet completed!
timeout /t 2
cls
goto inicioeng
)
if %opcao% equ 4 (
mode 90,40
cls
@echo You have chosen to clean the temporary files *-*
@echo Please don't close the window!
timeout /t 2
cls
@echo off
del /s /f /q c:\windows\temp\*.*
rd /s /q c:\windows\temp
md c:\windows\temp
del /s /f /q C:\WINDOWS\Prefetch
del /s /f /q C:\Windows\SoftwareDistribution\Download
del /s /f /q %temp%\*.*
rd /s /q %temp%
md %temp%
rmdir /s /q c:\windows\tempor~1
md c:\windows\tempor~1
rmdir /s /q c:\windows\temp
md c:\windows\temp
rmdir /s /q c:\windows\tmp
md c:\windows\tmp
rmdir /s /q c:\windows\ff*.tmp
md c:\windows\ff*.tmp
rmdir /s /q c:\windows\history
md c:\windows\history
rmdir /s /q c:\windows\cookies
md c:\windows\cookies
rmdir /s /q c:\windows\recent
c:\windows\recent
rmdir /s /q c:\windows\spool\printers
md c:\windows\spool\printers
del c:\WIN386.SWP
cls
msg * Cleanup completed!
timeout /t 2
goto inicioeng
)
if %opcao% equ 5 (
cls
goto installeng1
)
if %opcao% equ x (
cls
@echo Closing the app... :(
timeout /t 2
exit
)
if %opcao% equ X (
cls
@echo Closing the app... :(
timeout /t 2
exit
)
if $opcao% equ ERRORLEVEL 1 (
cls
@echo You have chosen wrong :(
@echo Try again.
timeout /t 2
cls
goto inicioeng
)
:installeng1
cls
@echo off
color 1
@echo FOR WINDOWS 10 1709 (build 16299) OR MORE (VIEW YOUR WINDOWS VERSION WITH 'ABOUT WINDOWS')
timeout /t 2
cls
@echo off
start winver /min
@echo:
:installeng2
cls
mode 106,20
@echo off
color 1
@echo      FOR WINDOWS 10 1709 (build 16299) OR MORE (VIEW YOUR WINDOWS VERSION WITH 'ABOUT WINDOWS')
@echo ==========================================================================================================
@echo  [ 1 ] install MemReduct app
@echo  [ 2 ] install OperaGX
@echo  [ 3 ] install Google Chrome
@echo  [ 4 ] install GPU-Z
@echo  [ 5 ] install CPU-Z
@echo  [ 6 ] install CCleaner
@echo  [ 7 ] install 7zip or WinRar
@echo  [ 8 ] install All Visual C++ Version (2005 - 2022)
@echo  [ 9 ] more options
@echo  [ x ] return
@echo ==========================================================================================================
set /p installing=     Select: 
if %installing% equ 1 (
mode 90, 13
cls
@echo Install program in progress...
@echo off
winget install Henry++.MemReduct -s winget -h
goto installeng2
cls
)
if %installing% equ 2 (
mode 90, 13
cls
@echo Install program in progress...
@echo off
winget install Opera.OperaGX -s winget -h
goto installeng2
)
if %installing% equ 3 (
mode 90, 13
cls
@echo in progress...
@echo off
winget install Google.Chrome -s winget -h
goto installeng2
)
if %installing% equ 4 (
mode 90, 13
cls
@echo in progress....
@echo off
winget install TechPowerUp.GPU-Z -h
goto installeng2
)
if %installing% equ 5 (
mode 90, 13
cls
@echo in progress....
@echo off
winget install CPUID.CPU-Z -h
goto installeng2
)
if %installing% equ 6 (
mode 90, 13
cls
@echo in progress....
@echo off
winget install Piriform.CCleaner -h
goto installeng2
)
if %installing% equ 7 (
mode 90, 13
cls
@echo off
goto ziprar
)
if %installing% equ 8 (
mode 90, 13
cls
@echo in progress...
timeout /t 1
cls
@echo off
start https://download1519.mediafire.com/mnqczktpja0g/vq2xp24j34w4034/All+Visual+Studio+%28Extract+Here%29.rar
@echo 'Extract Here' .rar archive and execute a Installer.bat!
@echo Press any key to return
timeout /t -1
goto installeng2
)
if %installing% equ 9 (
cls
goto installmore
)
if %installing% equ x (
cls
goto inicioeng
)
if %installing% equ X (
cls
goto inicioeng
)
if %%installing% equ ERRORLEVEL 1 (
cls
@echo selection wrong!
@echo off
timeout /t 1
goto installeng2
)

